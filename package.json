{
    "name": "digicommerce",
    "version": "1.0.3",
    "description": "WordPress plugin to sell digital products and services easily.",
    "scripts": {
        "clean": "rm -rf assets/css/* assets/js/*",
        "clean:ds_store": "find . -name '.DS_Store' -type f -delete",
        "photoswipe": "mkdir -p ./assets/css/vendor && cleancss -o ./assets/css/vendor/photoswipe.css ./resources/css/vendor/photoswipe.css",
        "copy:vendor-js": "mkdir -p ./assets/js/vendor && cp -r ./resources/js/vendor/*.js ./assets/js/vendor/ 2>/dev/null || true",
        "dev:css-base": "tailwindcss -c ./tailwind.front.config.js -i ./resources/css/base.css -o ./assets/css/base.css --postcss --watch",
        "dev:css-front": "tailwindcss -c ./tailwind.front.config.js -i ./resources/css/front.css -o ./assets/css/front.css --postcss --watch",
        "dev:css-admin": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/admin.css -o ./assets/css/admin/admin.css --postcss --watch",
        "dev:css-pro-addons": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/pro-addons.css -o ./assets/css/admin/pro-addons.css --postcss --watch",
        "dev:css-wizard": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/wizard.css -o ./assets/css/admin/wizard.css --postcss --watch",
        "dev:css-review-notice": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/review-notice.css -o ./assets/css/admin/review-notice.css --postcss --watch",
        "dev:css-orders-admin": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/orders-admin.css -o ./assets/css/admin/orders-admin.css --postcss --watch",
        "dev:css-dashboard": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/dashboard.css -o ./assets/css/admin/dashboard.css --postcss --watch",
        "dev:css-reports": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/reports.css -o ./assets/css/admin/reports.css --postcss --watch",
        "dev:css-import-export": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/import-export.css -o ./assets/css/admin/import-export.css --postcss --watch",
        "dev:css-product-metaboxes": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/product-metaboxes.css -o ./assets/css/admin/product-metaboxes.css --postcss --watch",
        "dev:css-sidebar": "tailwindcss -i ./resources/css/blocks/sidebar.css -o ./assets/css/blocks/sidebar.css --postcss --watch",
        "dev:js-front": "esbuild ./resources/js/front/*.js --bundle --outdir=assets/js/front --loader:.js=jsx --watch",
        "dev:js-admin": "esbuild ./resources/js/admin/*.js --bundle --outdir=assets/js/admin --loader:.js=jsx --watch",
        "dev:js-blocks": "node build-blocks.js dev",
        "dev:js-sidebar": "esbuild ./resources/js/blocks/sidebar.js --bundle --outfile=assets/js/blocks/sidebar.js --loader:.js=jsx --watch",
        "dev:js-globals": "esbuild ./resources/js/blocks/globals.js --bundle --outfile=assets/js/blocks/globals.js --loader:.js=jsx --watch",
        "build:css-base": "tailwindcss -c ./tailwind.front.config.js -i ./resources/css/base.css -o ./assets/css/base.css --postcss --minify",
        "build:css-front": "tailwindcss -c ./tailwind.front.config.js -i ./resources/css/front.css -o ./assets/css/front.css --postcss --minify",
        "build:css-admin": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/admin.css -o ./assets/css/admin/admin.css --postcss --minify",
        "build:css-pro-addons": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/pro-addons.css -o ./assets/css/admin/pro-addons.css --postcss --minify",
        "build:css-wizard": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/wizard.css -o ./assets/css/admin/wizard.css --postcss --minify",
        "build:css-review-notice": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/review-notice.css -o ./assets/css/admin/review-notice.css --postcss --minify",
        "build:css-orders-admin": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/orders-admin.css -o ./assets/css/admin/orders-admin.css --postcss --minify",
        "build:css-dashboard": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/dashboard.css -o ./assets/css/admin/dashboard.css --postcss --minify",
        "build:css-reports": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/reports.css -o ./assets/css/admin/reports.css --postcss --minify",
        "build:css-import-export": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/import-export.css -o ./assets/css/admin/import-export.css --postcss --minify",
        "build:css-product-metaboxes": "tailwindcss -c ./tailwind.admin.config.js -i ./resources/css/admin/product-metaboxes.css -o ./assets/css/admin/product-metaboxes.css --postcss --minify",
        "build:css-sidebar": "tailwindcss -i ./resources/css/blocks/sidebar.css -o ./assets/css/blocks/sidebar.css --postcss --minify",
        "build:js-front": "esbuild ./resources/js/front/*.js --bundle --minify --outdir=assets/js/front --loader:.js=jsx",
        "build:js-admin": "esbuild ./resources/js/admin/*.js --bundle --minify --outdir=assets/js/admin --loader:.js=jsx",
        "build:js-blocks": "node build-blocks.js build",
        "build:js-sidebar": "esbuild ./resources/js/blocks/sidebar.js --bundle --minify --outfile=assets/js/blocks/sidebar.js --loader:.js=jsx",
        "build:js-globals": "esbuild ./resources/js/blocks/globals.js --bundle --minify --outfile=assets/js/blocks/globals.js --loader:.js=jsx",
        "watch": "npm run clean && npm run clean:ds_store && concurrently \"npm run photoswipe\" \"npm run copy:vendor-js\" \"npm run dev:css-base\" \"npm run dev:css-front\" \"npm run dev:css-admin\" \"npm run dev:css-pro-addons\" \"npm run dev:css-wizard\" \"npm run dev:css-review-notice\" \"npm run dev:css-orders-admin\" \"npm run dev:css-dashboard\" \"npm run dev:css-reports\" \"npm run dev:css-import-export\" \"npm run dev:css-product-metaboxes\" \"npm run dev:css-sidebar\" \"npm run dev:js-front\" \"npm run dev:js-admin\" \"npm run dev:js-blocks\" \"npm run dev:js-sidebar\" \"npm run dev:js-globals\"",
        "build": "npm run clean && npm run clean:ds_store && npm run photoswipe && npm run copy:vendor-js && npm run build:css-base && npm run build:css-front && npm run build:css-admin && npm run build:css-pro-addons && npm run build:css-wizard && npm run build:css-review-notice && npm run build:css-orders-admin && npm run build:css-dashboard && npm run build:css-reports && npm run build:css-import-export && npm run build:css-product-metaboxes && npm run build:css-sidebar && npm run build:js-front && npm run build:js-admin && npm run build:js-blocks && npm run build:js-sidebar && npm run build:js-globals",
        "pack": "npm run build && find . -name '.DS_Store' -type f -delete && zip -r digicommerce.zip . -x '*node_modules*' -x '*/node_modules/*' -x '*/.*' -x '*.DS_Store' -x '*__MACOSX*' -x 'package.json' -x 'package-lock.json' -x 'tailwind*.config.js' -x 'postcss.config.js' -x 'extract-classes.js' -x 'build-blocks.js' -x 'composer.json' -x 'composer.lock' -x 'CONTRIBUTING.md' -x 'README.md' -x '.gitattributes' -x '.gitignore' -x '.prettierignore' -x 'prettier.config.mjs' -x 'phpcs.xml' -x '.git/*' -x '.git/**/*'",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\""
    },
    "devDependencies": {
        "@wordpress/prettier-config": "^4.16.0",
        "chokidar": "^3.5.3",
        "autoprefixer": "^10.4.16",
        "clean-css-cli": "^5.6.2",
        "concurrently": "^8.2.2",
        "cssnano": "^6.0.1",
        "esbuild": "^0.19.11",
        "postcss": "^8.4.32",
        "postcss-cli": "^11.0.0",
        "postcss-import": "^15.1.0",
        "postcss-nested": "^6.0.1",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "tailwindcss": "^3.4.17"
    }
}
